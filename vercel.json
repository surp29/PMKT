{
  "version": 2,
  "builds": [
    {
      "src": "frontend/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/app.py"
    }
  ],
  "env": {
    "BACKEND_URL": "https://phanmemketoan-backend.onrender.com",
    "FLASK_ENV": "production"
  }
}
